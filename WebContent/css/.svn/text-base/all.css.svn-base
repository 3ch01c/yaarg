iframe {
	width: 100%;
	height: 100%;
	margin: 0px;
}

body {
	font-family: Verdana, Geneva, sans-serif;
	font-size: 12px;
	color: white;
	margin: 0px;
	padding: 0px;
}

table {
	font-family: Verdana, Geneva, sans-serif;
	font-size: 12px;
	margin: 0px;
	padding: 1px;
	border: 0px;
}

input {
	font-family: Verdana, Geneva, sans-serif;
}

.update {
	font-style: italic;
	font-size: 10px;
}

a {
	color: #FF0;
	text-decoration: none;
}

a:visited {
	color: #CC3;
}

a:hover {
	color: #6CF;
}

.transbox {
	background-color: black;
	opacity: 0.4;
	filter: alpha(opacity = 40);
}

.code {
	font-family: Courier New, Lucida Console;
}

#delete {
	position: absolute;
	right: 10px;
	top: 10px;
}

.footer {
	position: absolute;
	bottom: 0px;
	text-align: center;
}
